{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current",
          "esmodules": true
        },
        "bugfixes": true
      }
    ],
    "@babel/react"
  ],
  "plugins": [
    "babel-plugin-styled-components"
  ],
  "env": {
    "production": {
      "plugins": [
        ["transform-react-remove-prop-types", { "mode": "wrap" }]
      ]
    }
  }
}
